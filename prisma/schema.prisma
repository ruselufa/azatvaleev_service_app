// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserModel {
  id       Int    @id @default(autoincrement())
  email    String
  password String
  name     String
}

model PurchaseModel {
  id           Int    @id @default(autoincrement())
  gcPurchaseId Int
  email        String
  name         String
  gcUserId     Int
  productTitle String
  startAt      String
  finishAt     String
  period       String
  state        String
  purchase_ink String
}

model UserExceptionModel {
  id               Int    @id @default(autoincrement())
  gcUserId         Int
  email            String
  firstName        String
  lastName         String
  gcOrderId        Int
  positionsInOrder String
  createdAtOrder   String
  statusOrder      String
}
